<appdef>
    <appname>TERMINAL</appname>
    <equal>com.apple.Terminal</equal>
    <equal>iTerm</equal>
    <equal>net.sourceforge.iTerm</equal>
    <equal>com.googlecode.iterm2</equal>
</appdef>
<!-- Escapeでusに変更 -->
<list>
   <item>
       <name>with EISUU (escape, Ctrl_L + Space, Ctrl_L + bracket+left)</name>
       <identifier>private.vim_alfred_with_eisuu</identifier>
       <autogen>--KeyToKey-- KeyCode::ESCAPE, KeyCode::ESCAPE, KeyCode::JIS_EISUU</autogen>
       <autogen>--KeyToKey-- KeyCode::SPACE, ModifierFlag::CONTROL_L, KeyCode::SPACE, ModifierFlag::CONTROL_L, KeyCode::JIS_EISUU</autogen>
       <autogen>--KeyToKey-- KeyCode::BRACKET_LEFT, ModifierFlag::CONTROL_L, KeyCode::BRACKET_LEFT, ModifierFlag::CONTROL_L, KeyCode::JIS_EISUU</autogen>
       <autogen>--KeyToKey-- KeyCode::JIS_BRACKET_LEFT, VK_CONTROL, KeyCode::JIS_BRACKET_LEFT, VK_CONTROL, KeyCode::JIS_EISUU</autogen>
   </item>

<!-- スティッキーズ整理 -->
  <item>
      <name> Stickeys clean </name>
      <identifier>private.stickeys</identifier>
      <autogen>--KeyToKey-- KeyCode::F4, ModifierFlag::COMMAND_L | ModifierFlag::NONE,
          KeyCode::CURSOR_DOWN, KeyCode::CURSOR_DOWN, KeyCode::CURSOR_DOWN,
          KeyCode::CURSOR_DOWN, KeyCode::CURSOR_DOWN, KeyCode::CURSOR_DOWN,
          KeyCode::CURSOR_DOWN, KeyCode::CURSOR_DOWN, KeyCode::CURSOR_RIGHT,
          KeyCode::CURSOR_DOWN, KeyCode::CURSOR_DOWN, KeyCode::CURSOR_RIGHT,
          KeyCode::RETURN
      </autogen>
  </item>

<!-- terminal clean  -->
<item>
    <name> terminal clean</name>
    <identifier>private.terminalclean</identifier>
    <autogen>--KeyToKey-- KeyCode::L, ModifierFlag::COMMAND_L | ModifierFlag::NONE,
        KeyCode::C, KeyCode::L, KeyCode::E, KeyCode::A, KeyCode::R, KeyCode::RETURN
    </autogen>
</item>

<!-- <C-w> word delete  -->
<item>
    <name> word delete</name>
    <identifier>private.worddelete</identifier>
    <autogen>--KeyToKey-- KeyCode::W, ModifierFlag::CONTROL_L | ModifierFlag::NONE,
        KeyCode::DELETE, ModifierFlag::OPTION_L
    </autogen>
</item>

<!-- Fキーで矢印、DELETE、RETURNモード  -->
   <item>
       <name>Custom Vi mode (F+)</name>
       <identifier>remap.custom_vi_mode_f</identifier>
       <autogen>
           __KeyOverlaidModifier__
           KeyCode::F
           @begin
           KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_notsave_custom_vi_mode_f,
           @end
           @begin
           KeyCode::F
           @end
       </autogen>
   </item>
   <item hidden="true">
       <identifier vk_config="true">notsave_custom_vi_mode_f</identifier>
       <autogen>
           __BlockUntilKeyUp__ KeyCode::F
       </autogen>
       <autogen>__KeyToKey__ KeyCode::H, KeyCode::CURSOR_LEFT</autogen>
       <autogen>__KeyToKey__ KeyCode::J, KeyCode::CURSOR_DOWN</autogen>
       <autogen>__KeyToKey__ KeyCode::K, KeyCode::CURSOR_UP</autogen>
       <autogen>__KeyToKey__ KeyCode::L, KeyCode::CURSOR_RIGHT</autogen>
       <autogen>__KeyToKey__ KeyCode::O, KeyCode::DELETE</autogen>
       <autogen>__KeyToKey__ KeyCode::SEMICOLON, KeyCode::RETURN</autogen>
   </item>

<!-- ;キーでw, bモード  -->
   <item>
       <name>Custom Vi mode (semicoron+)</name>
       <identifier>remap.custom_vi_mode_semicoron</identifier>
       <autogen>
           __KeyOverlaidModifier__
           KeyCode::SEMICOLON
           @begin
           KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_notsave_custom_vi_mode_semicoron,
           KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_notsave_custom_vi_mode_semicoron_not_terminal,
           @end
           @begin
           KeyCode::SEMICOLON
           @end
       </autogen>
   </item>
   <item hidden="true">
       <identifier vk_config="true">notsave_custom_vi_mode_semicoron</identifier>
       <only>TERMINAL</only>
       <autogen>
           __BlockUntilKeyUp__ KeyCode::SEMICOLON
       </autogen>
       <autogen>__KeyToKey__ KeyCode::W, KeyCode::F, ModifierFlag::OPTION_L | ModifierFlag::NONE </autogen>
       <autogen>__KeyToKey__ KeyCode::B, KeyCode::B, ModifierFlag::OPTION_L | ModifierFlag::NONE </autogen>
   </item>
   <item hidden="true">
       <identifier vk_config="true">notsave_custom_vi_mode_semicoron_not_terminal</identifier>
       <not>TERMINAL</not>
       <autogen>
           __BlockUntilKeyUp__ KeyCode::SEMICOLON
       </autogen>
       <autogen>__KeyToKey__ KeyCode::W, KeyCode::CURSOR_RIGHT, ModifierFlag::OPTION_L | ModifierFlag::NONE </autogen>
       <autogen>__KeyToKey__ KeyCode::B, KeyCode::CURSOR_LEFT, ModifierFlag::OPTION_L | ModifierFlag::NONE </autogen>
   </item>
<!-- Dキーでダッシュモード(Fキーの5倍) -->
<!-- <でurl貼り付けに便利なDELETE機能もある -->
   <item>
       <name>Cursor dash mode (D+)</name>
       <identifier>remap.cursor_dash_mode</identifier>
       <autogen>
           __KeyOverlaidModifier__
           KeyCode::D
           @begin
           KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_notsave_cursor_dash_mode,
           @end
           @begin
           KeyCode::D
           @end
       </autogen>
   </item>
   <item hidden="true">
       <identifier vk_config="true">notsave_cursor_dash_mode</identifier>
       <autogen>
           __BlockUntilKeyUp__ KeyCode::D
       </autogen>
       <autogen>__KeyToKey__ KeyCode::H, KeyCode::CURSOR_LEFT, KeyCode::CURSOR_LEFT, KeyCode::CURSOR_LEFT,
           KeyCode::CURSOR_LEFT, KeyCode::CURSOR_LEFT</autogen>
       <autogen>__KeyToKey__ KeyCode::J, KeyCode::CURSOR_DOWN, KeyCode::CURSOR_DOWN, KeyCode::CURSOR_DOWN,
           KeyCode::CURSOR_DOWN, KeyCode::CURSOR_DOWN</autogen>
       <autogen>__KeyToKey__ KeyCode::K, KeyCode::CURSOR_UP, KeyCode::CURSOR_UP, KeyCode::CURSOR_UP,
           KeyCode::CURSOR_UP, KeyCode::CURSOR_UP</autogen>
       <autogen>__KeyToKey__ KeyCode::L, KeyCode::CURSOR_RIGHT, KeyCode::CURSOR_RIGHT, KeyCode::CURSOR_RIGHT,
           KeyCode::CURSOR_RIGHT, KeyCode::CURSOR_RIGHT</autogen>
       <autogen>__KeyToKey__ KeyCode::O, KeyCode::DELETE, KeyCode::DELETE, KeyCode::DELETE, KeyCode::DELETE</autogen>
       <autogen>__KeyToKey__ KeyCode::SEMICOLON, KeyCode::RETURN, KeyCode::RETURN, KeyCode::RETURN, KeyCode::RETURN</autogen>
       <autogen>__KeyToKey__ KeyCode::SPACE, KeyCode::SPACE, KeyCode::SPACE, KeyCode::SPACE, KeyCode::SPACE</autogen>
       <autogen>__KeyToKey__ KeyCode::COMMA, KeyCode::CURSOR_RIGHT, KeyCode::CURSOR_RIGHT, KeyCode::DELETE, KeyCode::DELETE</autogen>
   </item>
<!-- Sキーでマウス移動、クリック -->
   <item>
       <name> Mouse mode</name>
       <identifier>mouse_mode</identifier>
       <autogen>
           __KeyOverlaidModifier__
           KeyCode::S, ModifierFlag::NONE,
           @begin
           KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_notsave_mouse_mode
           @end
           @begin
           KeyCode::S
           @end
       </autogen>
   </item>
   <item hidden="true">
       <identifier vk_config="true">notsave_mouse_mode</identifier>
       <autogen>
           __BlockUntilKeyUp__ KeyCode::S
       </autogen>
       <autogen>__KeyToKey__ KeyCode::H, KeyCode::VK_MOUSEKEY_LEFT</autogen>
       <autogen>__KeyToKey__ KeyCode::J, KeyCode::VK_MOUSEKEY_DOWN</autogen>
       <autogen>__KeyToKey__ KeyCode::K, KeyCode::VK_MOUSEKEY_UP</autogen>
       <autogen>__KeyToKey__ KeyCode::L, KeyCode::VK_MOUSEKEY_RIGHT</autogen>
       <autogen>__KeyToKey__ KeyCode::SEMICOLON, PointingButton::LEFT</autogen>
       <autogen>__KeyToKey__ KeyCode::R, PointingButton::RIGHT</autogen>
   </item>

<!-- Aキーでウィンドウ位置変更(BetterTouchToolと連携) -->
   <item>
       <name>Window mode(A+)</name>
       <identifier>window_mode</identifier>
       <autogen>
           __KeyOverlaidModifier__
           KeyCode::A, ModifierFlag::NONE,
           @begin
           KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_notsave_window_mode
           @end
           @begin
           KeyCode::A
           @end
       </autogen>
   </item>
   <item hidden="true">
       <identifier vk_config="true">notsave_window_mode</identifier>
       <autogen>
           __BlockUntilKeyUp__ KeyCode::A
       </autogen>
       <autogen>__KeyToKey__ KeyCode::G, KeyCode::F7, ModifierFlag::OPTION_L</autogen>
       <autogen>__KeyToKey__ KeyCode::H, KeyCode::F8, ModifierFlag::OPTION_L</autogen>
       <autogen>__KeyToKey__ KeyCode::J, KeyCode::F9, ModifierFlag::OPTION_L</autogen>
       <autogen>__KeyToKey__ KeyCode::K, KeyCode::F10, ModifierFlag::OPTION_L</autogen>
       <autogen>__KeyToKey__ KeyCode::L, KeyCode::F11, ModifierFlag::OPTION_L</autogen>
       <autogen>__KeyToKey__ KeyCode::SEMICOLON, KeyCode::F12, ModifierFlag::OPTION_L</autogen>
   </item>
</list>

</root>
