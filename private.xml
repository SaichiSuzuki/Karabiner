<item>
    <name>Utility</name>
    <identifier>private.utility</identifier>
    <!-- vim <(command) -->
    <autogen>--KeyToKey-- KeyCode::V, ModifierFlag::CONTROL_L | ModifierFlag::COMMAND_L | ModifierFlag::NONE,
        KeyCode::A, ModifierFlag::CONTROL_L, KeyCode::V, KeyCode::I, KeyCode::M, KeyCode::SPACE, KeyCode::COMMA, ModifierFlag::SHIFT_L, KeyCode::9, ModifierFlag::SHIFT_L, KeyCode::E, ModifierFlag::CONTROL_L, KeyCode::0, ModifierFlag::SHIFT_L, KeyCode::RETURN
    </autogen>
    <!-- vim most recently file open -->
    <autogen>--KeyToKey-- KeyCode::K, ModifierFlag::CONTROL_L | ModifierFlag::COMMAND_L | ModifierFlag::NONE,
        KeyCode::V, KeyCode::I, KeyCode::M, KeyCode::RETURN, KeyCode::CURSOR_UP, KeyCode::BACKSLASH, KeyCode::K, ModifierFlag::SHIFT_L, KeyCode::ESCAPE
    </autogen>
    <!-- chmod 755 file -->
    <autogen>--KeyToKey-- KeyCode::C, ModifierFlag::CONTROL_L | ModifierFlag::COMMAND_L | ModifierFlag::NONE,
        KeyCode::A, ModifierFlag::CONTROL_L, KeyCode::C, KeyCode::H, KeyCode::M, KeyCode::O, KeyCode::D, KeyCode::SPACE, KeyCode::7, KeyCode::5, KeyCode::5, KeyCode::SPACE, KeyCode::E, ModifierFlag::CONTROL_L, KeyCode::RETURN
    </autogen>
    <!-- terminal copy -->
    <autogen>--KeyToKey-- KeyCode::P, ModifierFlag::CONTROL_L | ModifierFlag::COMMAND_L | ModifierFlag::NONE,
        KeyCode::A, ModifierFlag::CONTROL_L, KeyCode::E, KeyCode::C, KeyCode::H, KeyCode::O, KeyCode::SPACE, KeyCode::QUOTE,  KeyCode::E, ModifierFlag::CONTROL_L, KeyCode::QUOTE, KeyCode::SPACE, KeyCode::BACKSLASH, ModifierFlag::SHIFT_L, KeyCode::SPACE, KeyCode::P, KeyCode::B, KeyCode::C, KeyCode::O, KeyCode::P, KeyCode::Y, KeyCode::RETURN
    </autogen>
    <!-- vim git status -->
    <autogen>--KeyToKey-- KeyCode::S, ModifierFlag::CONTROL_L | ModifierFlag::COMMAND_L | ModifierFlag::NONE,
        KeyCode::V, KeyCode::I, KeyCode::M, KeyCode::SPACE, KeyCode::Z, KeyCode::RETURN, KeyCode::SPACE, KeyCode::G, KeyCode::G
    </autogen>
    <!-- vim ag(何かのキー(CURSOR_RIGHT)を事前に打つ必要がある) -->
    <autogen>--KeyToKey-- KeyCode::G, ModifierFlag::CONTROL_L | ModifierFlag::COMMAND_L | ModifierFlag::NONE,
        KeyCode::V, KeyCode::I, KeyCode::M, KeyCode::RETURN, KeyCode::CURSOR_RIGHT, KeyCode::SEMICOLON, KeyCode::A, KeyCode::G, KeyCode::SPACE, KeyCode::CURSOR_LEFT
    </autogen>
    <!-- use vim tcomment -->
    <autogen>--KeyToKey-- KeyCode::H, ModifierFlag::CONTROL_L, KeyCode::F12, KeyCode::F12</autogen>
    <!-- alfred input jis mode -->
    <!-- <autogen>&#45;&#45;KeyToKey&#45;&#45; KeyCode::J, ModifierFlag::CONTROL_L, ModifierFlag::COMMAND_L | ModifierFlag::NONE, KeyCode::J, ModifierFlag::CONTROL_L, ModifierFlag::COMMAND_L, KeyCode::JIS_KANA, KeyCode::SPACE, ModifierFlag::CONTROL_L, KeyCode::J, ModifierFlag::CONTROL_L, ModifierFlag::COMMAND_L, KeyCode::JIS_KANA</autogen> -->
    <!-- <autogen>&#45;&#45;KeyToKey&#45;&#45; KeyCode::U, ModifierFlag::CONTROL_L | ModifierFlag::NONE, KeyCode::J, ModifierFlag::CONTROL_L, ModifierFlag::COMMAND_L, KeyCode::JIS_KANA</autogen> -->
    <!-- <autogen>&#45;&#45;KeyToKey&#45;&#45; KeyCode::U, ModifierFlag::CONTROL_L | ModifierFlag::NONE, KeyCode::J</autogen> -->
</item>

